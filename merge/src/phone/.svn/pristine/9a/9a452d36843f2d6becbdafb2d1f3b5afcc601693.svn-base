body {font-family: Microsoft YaHei;}
ul{
    list-style: none;
}
i,em{font-style: normal;}
@font-face {
    font-family:'icon';
    src: url('../fonts/icon.eot');
    src: url('../fonts/icon.eot?#iefix') format('embedded-opentype'),
         /*url('../font/icon.woff') format('woff'),*/
         url('../fonts/icon.ttf') format('truetype'),
         url('../fonts/icon.svg#iconregular') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family:'icon2';
    src: url('../fonts/icon.eot');
    src: url('../fonts/icon.eot?#iefix') format('embedded-opentype'),
         url('../fonts/icon.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
.icon2{
     font-family:'icon2';
     font-weight: normal;
     font-style: normal;
}


/* 底部外层 */
.footer{padding-left: 15px;padding-right: 15px;background: #fff;}
/* 回到顶部 */
.gobackTop{height: 60px;line-height: 60px;border-bottom: 1px solid #aeaeae;margin-bottom: 27px;}
.gobackTop span{position: relative;font-size: 14px;margin-left: 15px;padding: 12px;}
.gobackTop span i{position: absolute;top:50%;left:-25px;width: 24px;height: 35px;margin-top:-18px;background:url("../images/gobackTop.png") no-repeat center/24px 34px;}
/* 电话按钮 */
.call-server{height: 40px;line-height: 40px;}
.server-phone{
    letter-spacing: 1px;color: #fff;border-radius: 5px;
    background: #ef7f12 url('../images/phone.png') no-repeat 10px center/20px 20px;color: #fff;margin: 0 auto;padding:10px;padding-left: 40px;
}
 /* 底部导航 */
#bottom-nav{margin-top: 20px;margin-bottom: 14px;}
#bottom-nav li{display: inline-block;color:#000;padding-right: 12px;padding-left: 12px;border-right: 1px solid #000;}
#bottom-nav li:nth-last-of-type(1){border-right: none}
#bottom-nav li a{display: inline;padding: 0;color: #333;}
/* 版权 */
.copyright{color: #b8b8b8;font-size: 12px;margin-bottom: 4px;
 }



 /* pc端footer样式 */
.footer-pc{background: #fff;}
.footer-pc ul{padding-top: 50px;padding-bottom: 50px;}
.footer-pc Li h5{font-size: 18px;margin-bottom: 20px;}
.footer-pc Li a{padding: 4px 15px 4px 0;color:#777;font-size: 12px;}
.footer-pc ul img{width: 100px;height: 100px;}
.footer-pc Li a:hover{background: none;color:red;}


/*copyright-----------------------------------*/
.copyright-pc { /*background:#f8f8f8;*/}
.copyright-pc .content > div{ padding:4% 0; margin:0 20px; border-top:1px solid #e3e3e3}
.copyright-pc p{width:540px; margin:auto; font-size:12px; color:#777; text-align:center;font-size: 14px;}
.copyright-pc p span{display:inline-block;width:150px; height:20px; position:relative;}
.copyright-pc i{font-style: normal;}


/* 
    移动端导航
 */
/* 导航遮罩 */
.mask {display: none;z-index: 9999;position: fixed;top: 0;left: 0;right: 0;bottom: 0;background: rgba(0, 0, 0, 0.3);
}
/* 移动端导航按钮 */
.navbar-default .navbar-toggle .icon-bar { background: #fff;}
#m-nav {
    border: none;
    background: none;
    position: absolute;
}
/*移动端头部logo */
#m-nav .navbar-brand {
    background: url("../images/logo.png") no-repeat center center/contain;
    width: 60%;
    margin-left: 24px;
}
#iscroll{overflow-y:scroll;height: 100%; }
#scrollWrap{height: 100%;width:50%;position: absolute;right:0;background: #000;z-index: 11111;
    transition: all 0.3s ease;
    transform: translateX(100%);
    max-height: 100%;}
/* 右侧滑动导航 */
#main-nav {
    width: 100%; 
    position: absolute;
    top: 0;
    right: 0;
    margin: 0;  
}

#scrollWrap.show {transform: translateX(0%);}
#main-nav > li{position: relative;}
#main-nav > li a:hover {background: #000;}
#main-nav a {color: #fff;}
#main-nav .glyphicon {position:absolute;top:0px;right:0px;color: #fff;width: 41px;height: 34px;line-height: 36px;}
/* 二级导航 */
#main-nav .child-nav {color: #afafaf;list-style: disc;}

#main-nav li.current >a {color: #fda833;}
#main-nav li.currentPlace > a{color:#fda833;}

#main-nav .child-nav li {
    height: 34px;
    line-height: 34px;
    overflow: hidden;
    transition: all 0.3s ease;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
}

#main-nav li.nosub li {height: 0;}

.mianNav-wrap {position: absolute;top: 0;right: 0;margin: 0;overflow: hidden;width: 100%;height: 100%;z-index: -1;
}
.mianNav-wrap.showNav{z-index: 10000;}

/* 
    pc端头部导航 
*/
#pc-nav{
    height: 86px;margin-bottom: 0;background:transparent;border: none;border-bottom: 1px solid rgba(255, 255, 255,0.2);transition:all 0.4s;-moz-transition:all 0.4s;-ms-transition:all 0.4s;-o-transition:all 0.4s;-webkit-transition:all 0.4s;
}
#pc-nav .pc-top-nav{margin-top: 18px;margin-left: 26px;}
#pc-nav .pc-top-nav >li{position: relative;margin-right: 4px;}

#pc-nav .pc-top-nav a{color:#fff;cursor: pointer;}
#pc-nav .pc-top-nav > li >a:hover{color: #fd1428;}
#pc-nav .pc-top-nav > li >a{font-size: 16px;}

#pc-nav .pc-top-nav .pc-nav-active{color: #fd1428;font-weight: bold;}
#pc-logo{
    margin-top: 16px;
  
}
.pc-lv2-nav{padding: 0;background: rgba(205,35,50,0.95);position: absolute;top: 68px;
    font-size: 12px;min-width: 100px;text-align: center;display: none;}
.pc-lv2-nav::after{
    position: absolute;
    top: -9px;
    left: 50%;
    margin-left: -5px;
    width: 0;
    height: 0;
    border-right: 10px solid transparent;
    border-bottom: 10px solid  rgba(205,35,50,0.95);
    border-left: 10px solid transparent;
    margin-left: -11px;
}
.pc-lv2-nav li a{ padding: 12px 15px;} 
.pc-lv2-nav li a:hover{background: rgba(171,10,24,0.75)} 

/* 侧边栏 联系我们*/
/*registerInfor-----------------------------------*/
.rightSideBar{width:40px; height:200px; position: fixed; right:15px; top:50%; margin:-150px 0 0 0; z-index:990;}
.rightSideBar > div{ cursor:pointer; }
.rightSideBar .contactUs { margin:0 0 10px -20px; border-left:20px solid rgba(0,0,0,0);}
.rightSideBar .contactUs > span{ display:block;padding:10px;line-height:1.8; color:#fff; background:#cd2332;text-align:center;font-size:14px;position:relative; z-index:2;}
.rightSideBar .contactUs > span:before{ display:block; width:20px; height:30px; font-family:'icon'; content:'\ea13'; font-size:24px; line-height:1.5}
.rightSideBar .contactInfor{ width:190px; position:absolute; top:-60px; right:60px; padding:15px 0; background:#fff;border:1px solid #e5e5e5;box-shadow:0 0 10px #ddd; z-index:0; opacity:1;transform:translateX(300px);-moz-transform:translateX(300px);-ms-transform:translateX(300px);-o-transform:translateX(300px);-webkit-transform:translateX(300px);transition:all cubic-bezier(1,0,0,1) 0.7s;-moz-transition:all cubic-bezier(1,0,0,1) 0.7s;-ms-transition:all cubic-bezier(1,0,0,1) 0.7s;-o-transition:all cubic-bezier(1,0,0,1) 0.7s;webkit-transition:all cubic-bezier(1,0,0,1) 0.7s;}
.rightSideBar .contactInfor a{ display:block; text-align:center; font-size:14px; padding:15px 0 0 0; color:#222;text-decoration: none;}
.rightSideBar .contactInfor a:before{ display:block;font-family:'icon'; font-size:52px; line-height:1}
.rightSideBar .contactInfor .ewm{display:block; text-align:center; font-size:14px;margin: 30px auto 15px auto;padding:20px 0 0 0;border-top: 1px solid #ddd;}
.rightSideBar .contactInfor .ewm img{ margin:10px auto;width:90px;height:90px;}
.rightSideBar .contactUs:hover .contactInfor{ opacity:1;transform:translateX(0);-moz-transform:translateX(0);-ms-transform:translateX(0);-o-transform:translateX(0);-webkit-transform:translateX(0);}

.rightSideBar .returnTop{padding:10px;background:#cd2332;position:relative; z-index:2}
.rightSideBar .returnTop:after{display:block; width:20px; height:20px; font-family:'icon'; content:'\ea12'; font-size:24px; text-align:center; color:#fff; font-weight:normal; line-height:1.3}
.rightSideBar .consultant:before{content:'\ea09'; color:#fda833}
.rightSideBar .tel:before{ content:'\ea10';color:#f4772c}
.rightSideBar .E-mail:before{ content:'\ea11';color:#cd2332}
.contactInfor a:hover{text-decoration: none;}
.nav_up{top:-100px; background:none}



/*registerInfor-----------------------------------*/
.registerInfor{ background:#222222}
.registerInfor .content .head h2 {font-size:28px; line-height:1.5;text-align:center; color:#fff}
.registerInfor .content .head h2:before,.registerInfor .content .head h2:after{ display:none}
.registerInfor .content .head h3 {font-size:24px; line-height:1.5;text-align:center; font-weight:normal;color:#fff}
.registerInfor .content form{ box-sizing:border-box; width:90% ; max-width:590px; margin:auto; padding:3% 0 0 0;}
.registerInfor .content form > div{ position:relative; padding:0 0 0 100px; margin:10px 0}
.registerInfor .content form > div span{ position:absolute; display:block; width:90px; padding:2% 0; left:0; top:0;color:#fff; font-size:18px}
.registerInfor .content form > div > div{ position:relative}
.registerInfor .content form > div div input{ padding:3% 15px; width:100%;font-size:16px; border:none; border-radius:4px}

.registerInfor .content form .btn{ padding:5% 0 0 0; margin:0}
.registerInfor .content form .btn input{ display:block;width:175px;margin:auto; padding:3% 0; background:#cd2332; border:none; color:#fff; font-size:16px; border-radius:4px}

@media screen  and (max-width: 965px){
    .child-nav a{
        display: block;
    }
}
@media (min-width:768px) and (max-width: 965px){
    .navbar-header{
        width: 100%;
        padding: 10px;
    }
    .navbar-toggle{
        display: block;
    }
    #scrollWrap{
        width: 220px;
        font-size: 16px;
    }
    .navbar-nav>li{
        float: none;
    }
}

@media screen and (min-width: 970px){
    .jumbotron{
        padding-left: 18px!important;
    }
    .jumbotron .container{
        position: relative;
    }
}

