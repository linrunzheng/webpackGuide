}

.usePlace .caption h3 {
    color: red;
    font-size: 16px;
}

.usePlace .seeMore {
    margin-top: 22px;
    padding-bottom: 34px;
    border-bottom: 1px solid #ddd;
}

.benefit_detail p {
    float: none;
    margin: 0 auto;
    line-height: 1.6;
}

/* 行业列表 */
.selectList {
    background: #f9f9f9;
    padding-bottom: 30px;
}

.selectList .row {
    padding-left: 5px;
    padding-right: 5px;
    margin-top: 30px
}
.selectList h5 {margin-bottom: 6px;color: #333;}
.selectList h4 {font-size: 16px;}
.selectContent {
    border-radius: 3px;
    padding: 0;
    margin-bottom: 10px;
}

.selectContent > div {
    padding-left: 15px;
    padding-right: 15px;
    width: 90%;
    margin: 0 auto;
    border: 1px solid #e0e0e0;
    background: #fff;
    border-radius: 3px;
    background: #fff;
}
.imgWrap {height: 77px;}
.imgWrap img {margin: 0 auto;max-height: 100%;}
/* 行业列表 */
/* 合作伙伴 */
.cooperate{padding-top: 25px;}
.cooperate h4 {
    padding-left: 40px;
    padding-right: 40px;
    font-size: 14px;
    line-height: 1.5;
    color: #666;
}
.cooperate ul {
    padding: 15px 0 22px ;
    text-align: center;
}
.cooperate li {
    display: inline-block;
    margin-bottom: 12px;
    margin-left: 10px;

}
.cooperate li img {
    height: 28px;
}
/* 合作伙伴 */

/* 申请使用 */
.apply-use {
    background: url('../images/index/form_bg.png') no-repeat center/cover;
    color: #fff;
    padding-top: 20px;
    padding-bottom: 16px;
}
.apply-use > div{margin:0 auto;float: none;margin: 0 auto;}

.apply-use p {
    letter-spacing: 1px;
    font-size: 16px;
}
.apply-use form{padding-top: 10px;}
.apply-use input {
    margin-bottom: 15px;
    border-radius: 1px;
    height: 40px;
}
.apply-use label{
    font-weight: normal;
    color: #ef4747;
    margin-bottom: 10px;
}

.apply-use input::-webkit-input-placeholder {
    color: #aeaeae;
    font-family: "Microsoft YaHei";
}

.apply-btn {
    background: #cd2332;
    color: #fff;
    border: none;
}
/* 申请使用 */


/* 中間輪播 */

.tabContent > div{margin: 0 auto;overflow: hidden;padding: 0;}
.tabContent ul{width: 400%;position: relative;}
.tabContent li{float: left;width: 25%;}
.tabContent li .left{float: left;width: 50%;padding:0px;height: 100%;box-sizing: border-box;}
.tabContent li .right{float: left;width: 50%;padding:40px 80px;height: 100%;background: #cd2332;color: #fff;box-sizing: border-box;font-size: 16px;}
.tabContent li .right h3{font-size: 28px;}
.tabContent li .right a{color: #fff;width: 170px;height: 50px;line-height: 50px;display: inline-block;background: #fda833;text-align: center;margin-top: 30px;border-radius: 5px;}
.tabContent li .right h3{margin-bottom: 32px;}
.contList{height: 350px;}
.contList > div{height: 350px;}
.contList .left img{margin: 0 auto;max-width: 82%;padding-top: 10px;}

.icon-img,.icon2-img{text-align: center;}
.icon-img::before{font-family: icon2;font-size: 130px;}
.icon2-img::before{font-family: icon2;font-size: 50px;line-height: 2;}

.icon-img1::before{content: '\ea19';color: #c00c0c;}
.icon-img2::before{content: '\ea20';color: #f4772c;}
.icon-img3::before{content: '\ea23';color: #fda833;}
.icon-img4::before{content: '\ea25';color: #c00c0c;}
.icon-img5::before{content: '\ea21';color: #f4772c;}
.icon-img6::before{content: '\ea24';color: #fda833;}

.icon2-img1::before{content: '\ea38';color: #c00c0c;}
.icon2-img2::before{content: '\ea39';color: #c00c0c;}
.icon2-img3::before{content: '\ea40';color: #c00c0c;}
.icon2-img4::before{content: '\ea41';color: #c00c0c;}
.icon2-img5::before{content: '\ea42';color: #c00c0c;}
.icon2-img6::before{content: '\ea43';color: #c00c0c;}




