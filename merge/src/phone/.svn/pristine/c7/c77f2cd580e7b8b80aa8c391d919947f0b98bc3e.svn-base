@media screen and (max-width: 960px){
    .usePlace img{width: 75%;}
    .footBottom{height: 450px;overflow: hidden;}
    .swiper-wrapper{max-height: 450px!important;}
    .footBottom .swiper-container{width:100%;margin:auto;position:relative;text-align:center;overflow:hidden;}
    .swiper-wrapper{width: 400%;}
    .footBottom .swiper-container .swiper-slide{float:left;width:25%;height: 450px!important;position: relative;}
    .footBottom .swiper-container .swiper-slide:after{ display:block; content:''; clear:both}

    .swiper0{background:url("../images/index/banner-sj-m.jpg") no-repeat center/cover}
    .swiper1{background:url("../images/index/banner-dell-m.jpg") no-repeat center/cover}
    .swiper2{background:url("../images/index/banner-dx-m.jpg") no-repeat center/cover}
    .swiper3{background:url("../images/index/banner-product-m.jpg") no-repeat center/cover}

    .swiper-container strong{position: absolute;width: 139px;height: 38px;background: #cd2332;bottom: 0;line-height: 38px;text-align: center;color: #fff;font-weight: normal;left: 50%;margin-left: -70px; }
    .swiper-container strong a{color: #fff;display: block;width: 100%;height: 100%;}
     .swiper-container .swiper3 strong a{text-decoration: underline;}
    .swiper-container .know-more0{bottom: 100px;}
    .swiper-container .know-more1{bottom: 100px;background: #fda833;}
    .swiper-container .know-more2{bottom: 200px;}
    .swiper-container .know-more3{background: none;bottom: 76px;}
    .swiper3 h5{  margin-top: 100px;color: #fda833;font-size: 38px;letter-spacing: 6px;font-weight: bolder;}
    .swiper3  p{color: #fff;font-size: 16px;margin-top: 15px;margin-bottom: 14px;letter-spacing: 1px;}

    .usePlace .benefit_detail p{text-align: center;}

}
