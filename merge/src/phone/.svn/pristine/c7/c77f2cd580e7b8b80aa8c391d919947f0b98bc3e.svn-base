
.thumbnail {border: none;}
.mainContent { margin-top: 20px;}
.mainContent img {margin: 0 auto;}
.img-wrap {
    float: none;
    margin: 0 auto;
}

.intro  {
    color: #000;
    letter-spacing: 2px;
}
.intro p{
    padding: 20px;
    text-align: center;
}
.intro p span{
    display: inline-block;
    max-width: 970px;
}


.intro h3 {
    position: relative;
    margin-top: 40px;
    margin-bottom: 26px;
}

.intro h3 span {
    color: #a3a3a3;
