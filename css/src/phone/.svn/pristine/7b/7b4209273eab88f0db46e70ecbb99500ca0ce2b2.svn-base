<script type="text/javascript" src="public/layer/layer.js"></script>
<script type="text/javascript" src="public/js/jquery-1.11.2.min.js"></script>
<script type="text/javascript" src="public/js/jquery.validate.js"></script>
<script type="text/javascript" src="public/js/additional-methods.js"></script>
<script type="text/javascript" src="public/js/messages_zh.min.js"></script>

<script type="text/javascript">
/*
 * 选中状态
 */
$(document).ready(function () {
	validateForm();  //校验必输字段
});

/**
 * 校验
 */
function validateForm(){
	$("#channelAgent_form").validate({
		rules: {
			enterprise: {
			      required: true,
			      rangelength: [1,100]
			 },
			 userName: {
				 required: true,
			     rangelength: [1,100]
			},
			phoneNumber : {
	            required : true,
	            isMobile : true
	        },
	        email: {
				required: true,
				isEmail : true
			}
		},
		messages: {
			enterprise: {
			    required: "请输入您的企业名称",
			    rangelength: "企业名称长度1~100位"
			},
			userName: {
				required: "请输入您的姓名",
				rangelength: "姓名长度1~100位"
			},
			phoneNumber : {
				required : "请输入您的联系电话"
			},
			email: {
				required: "请输入您的联系邮箱"
			}
		}
	});
}

//验证邮件格式
jQuery.validator.addMethod(
		"isEmail", 
		function(value, element) {  
			if(value != null && value != "" ){
				return isTrueEmail(value);
			}else{
				return true;
			}
		},
		"请输入有效的电子邮件地址");


function isTrueEmail(str){
    var reg = /^(\w)+(\.\w+)*@([\w-])+((\.\w+)+)$/;
    return reg.test(str);
}

// 手机号码验证    
jQuery.validator.addMethod(
		"isMobile", 
		function(value, element) {    
		  var length = value.length;    
		  return this.optional(element) || (length == 11 && /^(((13[0-9]{1})|(15[0-9]{1})|(17[0-9]{1})|(18[0-9]{1}))+\d{8})$/.test(value));    
		},
		"请输入正确的手机号码！");

/**
 * 【登记试用】按钮
 */
 $("#btn_channelAgent").click(function(){
	 
	//校验数据
	if(!$("#channelAgent_form").valid()){
		return;
	}
	
	//点击【确定】按钮之后，显示遮罩层
    var index1 = layer.load(1, {
        shade: [0.1,'#000'] //0.1透明度的白色背景
    });
	
    $.ajax({ 
		type: "post", 
        url: "cloud/agents",
        datatype: "json",
        data: $("#channelAgent_form").serialize(),
        statusCode : {
		   201 : function(){
			   layer.alert("注册成功！",{"title":"提示"},  function(index){
	        		$("input[type=reset]").trigger("click");
	    			layer.close(index);
	    			layer.close(index1);
	    		});
		   },
		   500 : function(){
			   layer.alert("注册失败！",{"title":"提示"},  function(index){
	    			layer.close(index);
	    			layer.close(index1);
	    	   });
		   }
		}
    });
 });
</script>

