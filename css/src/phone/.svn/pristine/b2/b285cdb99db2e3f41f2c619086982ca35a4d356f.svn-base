<script type="text/javascript">
/**
 * 证书编号查询
 */
$("#certificateNoQuery").click(function() {

    //获取【证书编号】
    var certificateNo = $("#certificateNo").val();
    $.ajax({
        type: "get",
        url: "cloud/yunAppOrg/" + certificateNo,
        datatype: "json",
        // data:{"certificateNo":certificateNo},
        statusCode: {
            200: function(data) {
                if (data != "") {
                    $(".nofind").hide();
                    $("#companyName").text(data.name); //公司名称
                    $("#name").text(data.contacts); //联系人
                    $("#phoneNumber").text(data.phone); //联系电话
                    $("#email").text(data.email); //联系邮箱
                    $(".searchKey .showKey").show();
                }
            },
            404: function() {
                $(".searchKey .showKey").hide();
                $(".nofind").html('权利证明编号查询失败，您输入的权利证明编号不存在，请重新查询。').show();
                $("#companyName").text(""); //公司名称
                $("#name").text(""); //联系人
                $("#phoneNumber").text(""); //联系电话
                $("#email").text(""); //联系邮箱
            },
            500: function() {
                $(".searchKey .showKey").hide();
                $(".nofind").html('查询失败，请重新查询。').show();
                $("#companyName").text(""); //公司名称
                $("#name").text(""); //联系人
                $("#phoneNumber").text(""); //联系电话
                $("#email").text(""); //联系邮箱
            }
        }
    });
});
</script>

